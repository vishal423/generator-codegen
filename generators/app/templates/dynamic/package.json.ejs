{
  "name": "<%=npmPackageName%>",
  "version": "0.0.0",
  "description": "<%=description%>",
  "homepage": "https://github.com/<%=githubUsername%>/<%=npmPackageName%>",
  "author": {
    "name": "<%=name%>",
    "email": "<%=email%>",
    "url": "https://github.com/<%=githubUsername%>/<%=npmPackageName%>"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    <%_ if(type === 'jhipster-module') { _%>
    "jhipster-module",
    "jhipster-5",
    "jhipster-6",
    <%_ } _%>
    "yeoman-generator"
  ],
  "devDependencies": {
    "coveralls": "^3.0.4",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-prettier": "^3.1.0",
    "fs-extra": "^8.0.1",
    "husky": "^2.4.1",
    "jest": "^24.8.0",
    "lint-staged": "^8.2.1",
    "prettier": "^1.18.2",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^2.0.0"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "figlet": "^1.2.3",
    <%_ if(type === 'jhipster-module') {_%>
    "generator-jhipster": ">=5.0.0",
    <%_ } _%>
    "yeoman-generator": "^4.0.1"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/*.[j]s"
    ]
  },
  "scripts": {
    "pretest": "eslint .",
    "test": "jest --coverage && cat ./coverage/lcov.info | coveralls"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": "<%=githubUsername%>/<%=npmPackageName%>",
  "license": "MIT"
}
