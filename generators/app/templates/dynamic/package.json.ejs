    "url": "https://github.com/<%=githubUsername%>/generator-<%=generatorName%>"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    <%_ if(type === 'jhipster-module') { _%>
    "jhipster-module",
    "jhipster-5",
    <%_ } _%>
    "yeoman-generator"
  ],
  "devDependencies": {
    "coveralls": "^3.0.2",
    "eslint": "^5.9.0",
